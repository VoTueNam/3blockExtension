{
  "name": "3Block",
  "description": "Capstone project Group Spring 2022 semester | Protect your browser with 3Block. Using API connect to Black List and White list. Blocking malicious website or URL. Children Protection: prevent your children access 18+ content on web browser with API and Computer Vising. This is a free product and our team's graduation project.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "downloads",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "run_at": "document_idle",
    "all_frames": false,
    "js": [
      "content.js"
    ]
  }],
  "action": {
    "default_title": "3 Block",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/iconLogo_16.png",
      "32": "/images/iconLogo_32.png",
      "48": "/images/iconLogo_48.png",
      "128": "/images/iconLogo_128.png"
    }
  },
  "icons": {
    "16": "/images/iconLogo_16.png",
    "32": "/images/iconLogo_32.png",
    "48": "/images/iconLogo_48.png",
    "128": "/images/iconLogo_128.png"
  },
  "web_accessible_resources": [{
    "resources": [
      "404.html",
      "children.html",
      "childrenList.html"
    ],
    "matches": [
      "<all_urls>"
    ]
  }],
  "options_page": "/custom/index.html"
}